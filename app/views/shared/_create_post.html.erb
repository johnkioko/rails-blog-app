<%= stylesheet_link_tag 'form', media: 'all', 'data-turbolinks-track': 'reload' %>

<%= form_with model: @posts do |form| %>
  <div class="form-group">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div> 
  <div class="form-group">
    <%= form.label :text %>
    <%= form.text_field :text %>
  </div>
  <div class="form-group">
    <%= form.submit "Create Post" %>
  </div>
<% end %>